<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.webServer>
        <rewrite>
          <rules>
            <rule name="Rewrite to public request to profile page">
              <match url="^resume/([_0-9a-z-]+)" />
              <action type="Rewrite" url="profile.php?u={R:1}" />
            </rule>
          </rules>
        </rewrite>
    </system.webServer>
</configuration>